    "target": "ES2017"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
, "src/components/views/User/Dashboard/Dashboard.jsx", "src/components/views/User/Category/DetailCategory/DetailCategory.jsx", "src/components/views/User/Category/DetailCategory/useDetailCategory.js", "src/components/views/User/Category/DetailCategory/InfoTab/InfoTab.jsx", "src/components/views/User/Category/DetailCategory/InfoTab/useInfoTab.js", "src/components/views/User/Category/DetailCategory/IconTab/IconTab.jsx", "src/components/views/User/Category/DetailCategory/IconTab/useIconTab.js"  ],
  "exclude": [
